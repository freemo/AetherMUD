<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
<html>
<head>
	<title>System Resources</title>
	<script language=JavaScript>
	<!--
		function Delete()
		{
			if(confirm('Delete this user?! Confirm!'))
			{
				if(confirm('Press OK to Delete this user!'))
				{
					document.RESOURCES.action='playerdo.cmvp';
					document.RESOURCES.FUNCTION.value='DELETE';
					document.RESOURCES.PLAYER.value='@PlayerID@';
					document.RESOURCES.submit();
				}
			}
		}
		function Boot()
		{
			if(confirm('Boot this user off the system?'))
			{
				document.RESOURCES.action='playerdo.cmvp';
				document.RESOURCES.FUNCTION.value='BOOT';
				document.RESOURCES.PLAYER.value='@PlayerID@';
				document.RESOURCES.submit();
			}
		}
		function BanIP()
		{
			if(confirm('Ban this IP Address?'))
			{
				document.RESOURCES.action='playerdo.cmvp';
				document.RESOURCES.FUNCTION.value='BANBYIP';
				document.RESOURCES.PLAYER.value='@PlayerID@';
				document.RESOURCES.submit();
			}
		}
		function Ban()
		{
			if(confirm('Ban this Player?'))
			{
				document.RESOURCES.action='playerdo.cmvp';
				document.RESOURCES.FUNCTION.value='BANBYNAME';
				document.RESOURCES.PLAYER.value='@PlayerID@';
				document.RESOURCES.submit();
			}
		}
	//-->
	</script>
</head>
<body BGCOLOR=BLACK>
<P ALIGN=RIGHT><INPUT TYPE=BUTTON NAME=BACK VALUE="Close" ONCLICK="javascript:window.close();"></P>
<FORM NAME=RESOURCES TARGET=_top METHOD=GET ACTION="resources.cmvp">
<INPUT TYPE=HIDDEN NAME=AUTH VALUE="@Authenticate?AUTH@">
<INPUT TYPE=HIDDEN NAME=PLAYER VALUE="">
<INPUT TYPE=HIDDEN NAME=FUNCTION VALUE="">
<TABLE WIDTH=100% BORDER=1 CELLSPACING=0 CELLPADDING=0>
<TR>
	<TD WIDTH=60%>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=33%><FONT COLOR=YELLOW>Name: </FONT></TD>
		<TD WIDTH=66%><FONT COLOR=WHITE><B>@PlayerData?NAME@</B></FONT>@if?PlayerOnline?=true@<FONT COLOR=YELLOW>&nbsp;&nbsp;&nbsp; *ONLINE*</FONT>@endif@</TD>
	</TR>
	</TABLE>
	</TD>
	<TD WIDTH=40%><FONT COLOR=YELLOW>Race: </FONT><FONT COLOR=WHITE>@PlayerData?GENDER@ @PlayerData?RACE@</FONT></TD>
</TR>
<TR>
	<TD WIDTH=100% COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Description: </FONT></TD>
		<TD WIDTH=80%><FONT COLOR=WHITE><B>@PlayerData?DESCRIPTION@</B></FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD WIDTH=100% COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Level: </FONT>&nbsp;&nbsp;<FONT COLOR=WHITE>@PlayerData?LEVEL@</FONT></TD>
		<TD WIDTH=80%><FONT COLOR=YELLOW>Classes: </FONT><FONT COLOR=WHITE>@PlayerData?CLASSES@</FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Hit Points: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?HITPOINTS@</FONT><FONT COLOR=YELLOW>/</FONT><FONT COLOR=WHITE>@PlayerData?BASEHITPOINTS@</FONT></TD>
		<TD WIDTH=20% ALIGN=RIGHT><FONT COLOR=YELLOW>Stats:&nbsp;&nbsp;&nbsp;</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Str</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?STRENGTH@</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Int</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?INTELLIGENCE@</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Dex</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?DEXTERITY@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Mana: </FONT></TD>
		<TD WIDTH=40% COLSPAN=2><FONT COLOR=WHITE>@PlayerData?MANA@</FONT><FONT COLOR=YELLOW>/</FONT><FONT COLOR=WHITE>@PlayerData?BASEMANA@</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Wis</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?WISDOM@</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Con</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?CONSTITUTION@</FONT></TD>
		<TD WIDTH=7% ALIGN=LEFT><FONT COLOR=YELLOW>Chr</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?CHARISMA@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Movement: </FONT></TD>
		<TD WIDTH=80% COLSPAN=8><FONT COLOR=WHITE>@PlayerData?MOVEMENT@</FONT><FONT COLOR=YELLOW>/</FONT><FONT COLOR=WHITE>@PlayerData?BASEMOVEMENT@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Alignment: </FONT></TD>
		<TD WIDTH=80% COLSPAN=8><FONT COLOR=WHITE>@PlayerData?ALIGNMENTSTRING@ (@PlayerData?ALIGNMENT@)</FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Height: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?HEIGHT@</FONT> <FONT COLOR=YELLOW>inches tall</FONT></TD>
		<TD WIDTH=40% COLSPAN=2><FONT COLOR=YELLOW>Riding: </FONT> <FONT COLOR=WHITE>@PlayerData?RIDING@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Weight: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?WEIGHT@</FONT> <FONT COLOR=YELLOW>pounds</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Practice Sessions: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?PRACTICES@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Carrying: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?ENCUMBRANCE@</FONT><FONT COLOR=YELLOW>/</FONT><FONT COLOR=WHITE>@PlayerData?MAXCARRY@</FONT><FONT COLOR=YELLOW> pounds</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Training Sessions: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?TRAINS@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Age: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?HOURS@</FONT><FONT COLOR=YELLOW> hours online</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Total Experience: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?EXPERIENCE@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Gold: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?MONEY@</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Exp. To Level: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?EXPERIENCELEVEL@</FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Combat Info: </FONT></TD>
		<TD WIDTH=20%><BR></TD>
		<TD WIDTH=20% ALIGN=RIGHT><FONT COLOR=YELLOW>Save Vs:&nbsp;&nbsp;&nbsp;</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Fire</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?FIRE@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Cold</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?COLD@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Water</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?WATER@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Attack: </FONT></TD>
		<TD WIDTH=40% COLSPAN=2><FONT COLOR=WHITE>@PlayerData?ATTACK@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Para</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?PARALYSIS@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Dise</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?DISEASE@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Magic</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?MAGIC@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Damage: </FONT></TD>
		<TD WIDTH=40% COLSPAN=2><FONT COLOR=WHITE>@PlayerData?DAMAGE@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Pois</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?POISON@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Acid</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?ACID@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Gas</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?GAS@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Defense: </FONT></TD>
		<TD WIDTH=40% COLSPAN=2><FONT COLOR=WHITE>@PlayerData?ARMOR@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Dead</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?UNDEAD@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Elec</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?ELECTRICITY@</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=YELLOW>Mind</FONT></TD>
		<TD WIDTH=7%><FONT COLOR=WHITE>@PlayerData?MIND@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Wimpy </FONT></TD>
		<TD WIDTH=80% COLSPAN=8><FONT COLOR=WHITE>@PlayerData?WIMP@</FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD COLSPAN=2>
	<TABLE WIDTH=100% BORDER=0 CELLSPACING=0 CELLPADDING=0>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Location: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?LOCATION@</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Leige: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?LEIGE@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Start Room: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?STARTROOM@</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Clan: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?CLAN@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Email: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?EMAIL@</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Deity: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?DEITY@</FONT></TD>
	</TR>
	<TR>
		<TD WIDTH=20%><FONT COLOR=YELLOW>LastIP: </FONT></TD>
		<TD WIDTH=40%><FONT COLOR=WHITE>@PlayerData?LASTIP@</FONT></TD>
		<TD WIDTH=20%><FONT COLOR=YELLOW>Quest Points: </FONT></TD>
		<TD WIDTH=20%><FONT COLOR=WHITE>@PlayerData?QUESTPOINTS@</FONT></TD>
	</TR>
	</TABLE>
	</TD>
</TR>
<TR>
	<TD WIDTH=100% COLSPAN=2><FONT COLOR=YELLOW>Equipment: </FONT><FONT COLOR=WHITE>@PlayerData?INVENTORY@</FONT></TD>
</TR>
<TR>
	<TD WIDTH=100% COLSPAN=2><FONT COLOR=YELLOW>Abilities: </FONT><FONT COLOR=WHITE>@AbilityNext?reset@ @loop@ @AbilityNext@ @AbilityName@ / @back@</FONT></TD>
</TR>
</TABLE>
<P>
	<TABLE WIDTH=100% BORDER=0>
	<TR>
	<TD WIDTH=20% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=BACK VALUE="Close" ONCLICK="javascript:window.close();">
	</TD>
	<TD WIDTH=20% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=DELETE VALUE="Delete" ONCLICK="Delete();">
	</TD>
	<TD WIDTH=20% ALIGN=CENTER>
	@if?PlayerOnline?=true@
	<INPUT TYPE=BUTTON NAME=BOOT VALUE="Boot Player" ONCLICK="Boot();">
	@endif@
	</TD>
	<TD WIDTH=20% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=BOOT VALUE="Ban Player" ONCLICK="Ban();">
	</TD>
	<TD WIDTH=20% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=BOOT VALUE="Ban IP Address" ONCLICK="BanIP();">
	</TD>
	</TR>
	</TABLE>
</FORM>
</BODY>
</HTML>
