<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
	<title>System Resources</title>
</head>

<body BGCOLOR=#FFCCCC>
<h3>CoffeeMud File Resources Management</h3><P>
<FORM NAME=RESOURCES TARGET=_top METHOD=GET ACTION="resources.cmvp">
	<CENTER><FONT COLOR=RED><B>@ResourceMgr?DELETE@</B></FONT></CENTER>
	<BR>
	<TABLE WIDTH=100% BORDER=1>
	<TR><TD WIDTH=25% COLSPAN=4 BGCOLOR=#FF00CC ALIGN=CENTER><H2>Resources</H2></TD></TR>
	@ResourceMgr?RESET@
	@loop@
	<TR>
	@ResourceMgr?NEXT@<TD WIDTH=25%><INPUT TYPE=RADIO NAME=RESOURCE VALUE="@ResourceMgr@"><B>@ResourceMgr@</B></a></TD>
	@ResourceMgr?NEXT&EMPTYOK@<TD WIDTH=25%><INPUT TYPE=RADIO NAME=RESOURCE VALUE="@ResourceMgr@"><B>@ResourceMgr@</B></a></TD>
	@ResourceMgr?NEXT&EMPTYOK@<TD WIDTH=25%><INPUT TYPE=RADIO NAME=RESOURCE VALUE="@ResourceMgr@"><B>@ResourceMgr@</B></a></TD>
	@ResourceMgr?NEXT&EMPTYOK@<TD WIDTH=25%><INPUT TYPE=RADIO NAME=RESOURCE VALUE="@ResourceMgr@"><B>@ResourceMgr@</B></a></TD>
	</TR>
	@back@
	</TABLE>
	<P>
	<TABLE WIDTH=100% BORDER=0>
	<TR>
	<TD WIDTH=50% ALIGN=CENTER>
	<a href="admin/grinder/loggedin.cmvp">Back to previous menu</a>
	</TD>
	<TD WIDTH=50% ALIGN=CENTER>
	<INPUT TYPE=SUBMIT NAME="Delete selected" VALUE="Delete selected">
	</TD>
	</TABLE>
</BODY>
</HTML>
