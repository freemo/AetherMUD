		function Clicked(action)
		{
			document.CONTROL.action=action;
			document.CONTROL.submit();
		}
		function ClickedINI(action)
		{
			document.INIFORM.action=action;
			document.INIFORM.submit();
		}
		function inipage(action)
		{
			document.INIFORM.action='/grinder/control.cmvp#INICHANGES';
			document.INIFORM.INIPAGE.value=action;
			document.INIFORM.submit();
		}
		function Disable(checked,field)
		{
			document.CONTROL.action='/grinder/control.cmvp';
			document.CONTROL.QUERY.value='CHANGEDISABLE';
			document.CONTROL.FIELD.value=field;
			if(checked)	
				document.CONTROL.VALUE.value='';
			else	
				document.CONTROL.VALUE.value='on';
			document.CONTROL.submit();
		}
		function Reset(checked,field,value)
		{
			document.CONTROL.action='/grinder/control.cmvp';
			document.CONTROL.QUERY.value='RESET';
			document.CONTROL.FIELD.value=field;
			document.CONTROL.VALUE.value=value;
			document.CONTROL.submit();
		}
		function Query(checked,field,value)
		{
			document.CONTROL.action='/grinder/control.cmvp';
			document.CONTROL.QUERY.value='QUERY';
			document.CONTROL.FIELD.value=field;
			document.CONTROL.VALUE.value=value;
			document.CONTROL.submit();
		}
		function AddRemove(prepend,key,near,action)
		{
			if((prepend.length>0)&&(key.indexOf(prepend)==0))
			{
				alert('Do not include '+prepend+' in your tag.  It will be done automatically.');
				return;
			}
			if(key.length==0)
			{
				alert('You must enter a value.');
				return;
			}
			document.INIFORM.action='/grinder/control.cmvp#INICHANGES';
			document.INIFORM.CONTROL.value=action;
			document.INIFORM.KEY.value=prepend+key;
			document.INIFORM.NEAR.value=near;
			document.INIFORM.submit();
		}
		function openUp(named)
		{
			var strFeatures="top=200,left=50,resizeable=no,scrollbars=yes,height=400,width=650,alwaysRaised=yes,dependent=yes";
			w=window.open("/property.cmvp?ABILITY="+named,
			            "Ability",strFeatures, false);
		}
		function openUp2(named)
		{
			var strFeatures="top=200,left=50,scrollbars=yes,resizeable=no,height=400,width=650,alwaysRaised=yes,dependent=yes";
			w=window.open("/ahelp.cmvp?HELPTOPIC="+named,
