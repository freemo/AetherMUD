<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">

<html>
<head>
	<!-- tell spiders to sod off -->
	<meta name="ROBOTS" content="NOINDEX, NOFOLLOW">
	<link rel="STYLESHEET" TYPE="TEXT/CSS" HREF="coffeemud.css">
	<title>CoffeeMUD Areas</title>
	<SCRIPT LANGUAGE=JavaScript>
	<!--
	function openUp(named)
	{
		var strFeatures="top=200,left=50,resizeable=no,height=400,width=650,alwaysRaised=yes,dependent=yes";
		w=window.open("area.cmvp?AREA="+named,
		            "Ability",strFeatures, false);
	}
	//-->
	</SCRIPT>
</head>

<body>
<div align="center">
	<h3>Installed map areas</H3>
	<hr><br>

	<table border="1" align="center">
	@AreaNext?reset@
	@loop@
	<TR>
		@AreaNext@<TD><a href="javascript:openUp('@AreaName@');">@AreaName@</TD>
		@AreaNext?EMPTYOK@<TD><a href="javascript:openUp('@AreaName@');">@AreaName@</TD>
		@AreaNext?EMPTYOK@<TD><a href="javascript:openUp('@AreaName@');">@AreaName@</TD>
		@AreaNext?EMPTYOK@<TD><a href="javascript:openUp('@AreaName@');">@AreaName@</TD>
	</TR>
	@back@
	</TABLE>
	<br>
	<hr>
	<a href="/">back to top page</A>
	</div>
</body>
</html>
